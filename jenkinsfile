pipeline{ 
    agent any

     
 


    stages{
        stage('Checkout'){
            steps{
                git url: 'https://github.com/rohansaini-02/FinalEvalRepo', branch:'master'
                echo "Checkout Complete"
            }
        }

        
            stage('Build'){
            steps{
                sh 'mvn -B -DskipTests clean package'
            }
        }
            
            stage('Test'){
                steps{
                    bat 'mvn test'
                    echo 'Test Completed'
                }
            }


             stage('Test'){
            steps{
                sh 'mvn test'
            }
        }

        stage('Deliver'){
            steps{
                sh './scripts/deliver.sh'
            }
        }


  
    }

}


   