pipeline{

    
    agent any

    tools{
        maven 'maven'
        jdk 'JDK'  

    }
    

    stages{
        
            stage('Build'){
                steps{
                   bat 'mvn -B -DskipTests clean package'
                   echo 'Build Completed'
                }
            }
            
            stage('Test'){
                steps{
                    bat 'mvn test'
                    echo 'Test Completed'
                }
            }


                 stage('Deliver') {
            steps {
                script { 
                    sh './scripts/deliver.sh'
                }
            }
        }

  
    }

}


  
 

